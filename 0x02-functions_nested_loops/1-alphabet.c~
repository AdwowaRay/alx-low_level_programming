#include "main.h"
/**
 *main - Entry point
 *Return: Always 0 (Success)
 */
int main(void)
{
	/* prints the alphabet, in lowercase, followed by a new line. */
	print_alphabet();
	return (0);
}

void print_alphabet(void)
{
	int ch = 'a';
	
	while (ch <= 'z')
	{
		_putchar(ch);
		ch--;
	}
	_putchar('\n');
}
